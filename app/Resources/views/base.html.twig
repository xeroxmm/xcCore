            $.ajax({
                url: tURL ,
                success: function(html) {
                    if(typeof html.p !== 'undefined' && html.p !== null && typeof html.p.c !== 'undefined'){
                        if (typeof history.pushState !== "undefined") {
                            var obj = { dummy: 'char_1' };
                            history.pushState(obj, page_title, page_url);
                        }
                        html = html.p.c;
                        if(!(tURL in appended)){
                            appended[tURL] = true;
                            $('#linkLoadRecent').parent().remove();
                            if($('.xcInfBox.active').length){
                                $('.xcInfBox.active').append(html);
                            } else if($('#'+object).length){
                                $('#'+object).append(html);
                            } else if($("#grid-main").length){
                                $("#grid-main").append(html);
                            } else {
                                $("#img-grid-box").append(html);
                            }

