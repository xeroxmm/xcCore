{% htmlcompress %}
{% set user = FALSE %}
{# @var user \Symfony\Component\Security\Core\User\User #}
{# @var client \AppBundle\Security\User\Anonym\SessionHandler #}
{% set descr = descriptionString | default('') %}
{% set focusOn = app.request.cookies.get('x9001s') == '1' ? 'pop' : 'new' %}
<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="{{ asset('/public/css/bootstrap.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('/public/css/global.css') }}"/>
    <link rel="stylesheet" href="{{ asset('/css/custom.css') }}"/>
    {% block stylesheets %}{% endblock %}
    {% if exoclicks is defined %}
        <meta name="exoclick-site-verification" content="{{ exoclicks }}">
    {% endif %}
    {% if xhamster is defined %}
        <meta name="xhamster-site-verification" content="{{ xhamster }}"/>
    {% endif %}
    {% if hubtraffic is defined %}
        <meta name="hubtraffic-domain-validation" content="{{ hubtraffic }}"/>
    {% endif %}
    {% if yandex is defined %}
        <meta name="yandex-verification" content="{{ yandex }}"/>
    {% endif %}
    {% if themecolor is defined %}
        <meta name="theme-color" content="#{{ themecolor }}">
    {% endif %}
    {% if googleSiteVerification is defined %}
        <meta name="google-site-verification" content="{{ googleSiteVerification }}"/>
    {% endif %}
    {% if bing is defined %}
    <meta name="msvalidate.01" content="{{ bing }}"/>
    {% endif %}
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="referrer" content="origin">
    <meta name="RATING" content="RTA-5042-1996-1400-1577-RTA"/>
    <meta name="robots" content="follow,index"/>
    {% block metaLink %}{% endblock %}
    {% block metaIndex %}{% endblock %}
    <meta charset="UTF-8"/>
    <title>{% block title %}{% if homepage_title is defined %}{{ homepage_title }}{% else %}{{ brand }}{% endif %}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="author" content="{{ brand }}"/>
    {% if twitter is defined %}
        <meta name="twitter:title" content="{{ brand }}"/>
        {% if descriptionString is defined %}
            <meta name="twitter:description" content="{{ descr }}"/>{% endif %}
    {% endif %}

    <link rel="icon" href="{{ asset('/img/favicon.ico') }}">
    <meta name="description" content="{% block description %}{{ descr }}{% endblock %}"/>
    {% block meta %}{% endblock %}
    {% if juicyAdsPopUp is defined and juicyAdsPopUp %}
        <script type="text/javascript"
                src="https://js.juicyads.com/jp.php?c={{ juicyAdsPopUp }}&u=http%3A%2F%2Fwww.juicyads.rocks"></script>
    {% endif %}
    {% if exoclicksPopUp is defined %}
        <script type="text/javascript">
            var ad_idzone = "{{ exoclicksPopUp }}",
                ad_frequency_period = 720,
                ad_frequency_count = 1;
        </script>
        <script type="text/javascript" src="https://ads.exosrv.com/popunder1000.js"></script>
    {% endif %}
</head>
<body id="base" class="base">
{% include "navigationHead/navbarSidebar.html.twig" %}
